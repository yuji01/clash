name: Spotify
desc: 测试Spotify解锁

http:
  mitm:
    - "spclient.wg.spotify.com"
  script:
    - match: ^https:\/\/spclient\.wg\.spotify\.com\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$
      name: spotify
      type: response
      require-body: true
      timeout: 10
      
  script-providers:
    spotify:
      url: https://cdn.jsdelivr.net/gh/sve1r/Rules-For-Quantumult-X@develop/Scripts/Unlock/Spotify-proto.js
      interval: 86400
